   "source": [
    "# Decorators101\n",
    "\n",
    "装饰器：\n",
    "\n",
    "- 是一个callable对象\n",
    "- 以函数为参数"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "@decorate\n",
    "def target():\n",
    "    print(\"running target()\")\n",
    "    \n",
    "# 上面的装饰器用法，实际\n",
    "    \n",
    "def target():\n",
    "    print(\"running target()\")\n",
    "    \n",
    "target = decorate(target)"
   ]
  }
